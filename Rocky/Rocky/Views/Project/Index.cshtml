@model IEnumerable<Rocky.Models.Project>
    

    <div>
        <a asp-controller="Project" asp-action="Create" class="btn btn-primary">Create project</a>
        <p></p>
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Project name</th>
                    <th>Description</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                    {
                <tr>
                    <td width="5%">@obj.Id</td>
                    <td width="20%">@obj.Name</td>
                    <td width="30%">@obj.Description</td>
                </tr>
                    }
            </tbody>
        </table>
    </div>
